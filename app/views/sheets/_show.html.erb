<div class="partial sheets-show-partial">
  <%= link_to sheet_path(@sheet), method: :delete, data: { confirm: "Are you sure to delete this sheet?" } do %><span class="link_tool fa fa-trash-o"></span><% end %>
  <div class="link_tool fa fa-star-o"></div>
  <div class="link_tool fa fa-pencil"></div>

  <h3><%= sheet.title %></h3>

  <div>
    <% sheet.tags.each do |tag| %>
      <i><%= tag.title %></i>
    <% end %>
  </div>
  <p><%= markdown(sheet.body) %></p>
</div>
